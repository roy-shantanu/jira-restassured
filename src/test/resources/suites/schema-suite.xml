<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Schema Suite" verbose="1">
    <test name="All schema suites">
        <groups>
            <run>
                <include name="schema"/>
            </run>
        </groups>
        <packages>
            <package name=".*"/>
        </packages>
    </test>
</suite>